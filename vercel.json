{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["hkg1", "sin1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "@japanese-flashcard-api-url",
    "NEXT_PUBLIC_APP_NAME": "@japanese-flashcard-app-name",
    "NEXT_PUBLIC_DEFAULT_LANGUAGE": "@japanese-flashcard-default-language"
  }
}
